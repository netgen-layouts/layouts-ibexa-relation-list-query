{
    "name": "netgen/layouts-ibexa-relation-list-query",
    "description": "Netgen Layouts query based on relation list field type for Ibexa CMS",
    "license": "GPL-2.0-only",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://netgen.io"
        }
    ],
    "require": {
        "php": "^8.4",
        "netgen/layouts-ibexa": "^1.4"
    },
    "require-dev": {
        "netgen/layouts-coding-standard": "^2.0",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-strict-rules": "^2.0"
    },
    "config": {
        "allow-plugins": false
    },
    "scripts": {
        "phpstan": "@php vendor/bin/phpstan analyse -c phpstan.neon --level=8 --ansi lib bundle"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\Layouts\\Ibexa\\RelationListQuery\\": "lib/",
            "Netgen\\Bundle\\LayoutsIbexaRelationListQueryBundle\\": "bundle/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
